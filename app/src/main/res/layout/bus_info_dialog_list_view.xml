<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:paddingLeft="0.5dip"
    android:paddingRight="0.5dip"
    android:layout_height="match_parent"
    android:background="#FF797979">


    <TextView
        android:id="@+id/dialog_no"
        android:layout_width="100dp"
        android:layout_height="31dp"
        android:layout_gravity="center_vertical"
        android:layout_marginBottom="0.5dip"
        android:layout_marginLeft="0.5dip"
        android:gravity="center"
        android:textColor="#FF4F4F4F"
        android:background="#FFF9F9F9"
        android:text="1"/>

    <TextView
        android:id="@+id/dialog_busNo"
        android:layout_width="100dp"
        android:layout_height="31dp"
        android:layout_gravity="center_vertical"
        android:layout_marginBottom="0.5dip"
        android:layout_marginLeft="0.5dip"
        android:gravity="center"
        android:textColor="#FF4F4F4F"
        android:background="#FFF9F9F9"
        android:text="7"/>

    <TextView
        android:id="@+id/dialog_capacity"
        android:layout_width="100dp"
        android:layout_height="31dp"
        android:layout_gravity="center_vertical"
        android:layout_marginBottom="0.5dip"
        android:layout_marginLeft="0.5dip"
        android:background="#FFF9F9F9"
        android:textColor="#FF4F4F4F"
        android:gravity="center"
        android:text="8"/>

</TableRow>
